{
  "name": "teloquent",
  "version": "1.0.0",
  "description": "A TypeScript package for eloquent communication",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
