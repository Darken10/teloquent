{
  "name": "teloquent",
  "version": "1.0.3",
  "description": "An Eloquent-inspired ORM for TypeScript with PostgreSQL, MySQL and SQLite support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "teloquent": "./dist/cli/index.js"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./decorators": {
      "types": "./dist/decorators/index.d.ts",
      "require": "./dist/decorators/index.js",
      "default": "./dist/decorators/index.js"
    },
    "./migrations": {
      "types": "./dist/migrations/index.d.ts",
      "require": "./dist/migrations/index.js",
      "default": "./dist/migrations/index.js"
    },
    "./relations": {
      "types": "./dist/relations/index.d.ts",
      "require": "./dist/relations/index.js",
      "default": "./dist/relations/index.js"
    },
    "./scopes": {
      "types": "./dist/scopes/index.d.ts",
      "require": "./dist/scopes/index.js",
      "default": "./dist/scopes/index.js"
    },
    "./cli": "./dist/cli/index.js"
  },
  "typesVersions": {
    "*": {
      "decorators": [
        "dist/decorators/index.d.ts"
      ],
      "migrations": [
        "dist/migrations/index.d.ts"
      ],
      "relations": [
        "dist/relations/index.d.ts"
      ],
      "scopes": [
        "dist/scopes/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "migrate": "ts-node src/cli/index.ts migrate",
    "seed": "ts-node src/cli/index.ts seed",
    "generate:model": "ts-node src/cli/index.ts generate:model"
  },
  "keywords": [
    "typescript",
    "orm",
    "database",
    "eloquent",
    "active-record",
    "postgresql",
    "mysql",
    "sqlite",
    "knex"
  ],
  "author": "Darken10",
  "license": "MIT",
  "dependencies": {
    "knex": "^3.0.1",
    "pg": "^8.11.3",
    "mysql2": "^3.6.1",
    "sqlite3": "^5.1.6",
    "reflect-metadata": "^0.1.13",
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "dotenv": "^16.3.1",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "@types/jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0",
    "@types/pluralize": "^0.0.30"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Darken10/teloquent.git"
  },
  "bugs": {
    "url": "https://github.com/Darken10/teloquent/issues"
  },
  "homepage": "https://github.com/Darken10/teloquent#readme"
}
